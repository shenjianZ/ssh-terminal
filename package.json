{
  "name": "ssh-terminal",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:win": "powershell -ExecutionPolicy Bypass -File ./build-win.ps1",
    "tauri:android": "tauri android build --",
    "tauri:android:clean": "tauri android build -- --clean",
    "tauri:android:release:arm64": "tauri android build -- --release --target arm64-v8a",
    "tauri:android:release:x86_64": "tauri android build -- --release --target x86_64",
    "tauri:android:release:universal": "tauri android build -- --release --target universal",
    "tauri:android:debug:arm64": "tauri android build -- --debug --target arm64-v8a",
    "tauri:android:debug:x86_64": "tauri android build -- --debug --target x86_64",
    "tauri:android:debug:universal": "tauri android build -- --debug --target universal",
    "tauri:android:install:arm64": "pnpm run tauri:android:release:arm64 && adb install -r ./src-tauri/gen/android/app/build/outputs/apk/arm64-v8a/release/app-arm64-v8a-release.apk",
    "tauri:android:install:x86_64": "pnpm run tauri:android:release:x86_64 && adb install -r ./src-tauri/gen/android/app/build/outputs/apk/x86_64/release/app-x86_64-release.apk",
    "tauri:android:install:universal": "pnpm run tauri:android:release:universal && adb install -r ./src-tauri/gen/android/app/build/outputs/apk/universal/release/app-universal-release-signed.apk"
  },
